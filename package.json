{
  "name": "weather-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "yarn run git-cz",
    "dev": "next dev",
    "build": "next build",
    "prepare": "husky install",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint"
    ]
  },
  "packageManager": "yarn@3.6.4"
}
